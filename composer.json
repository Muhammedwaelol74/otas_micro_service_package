{
    "name": "solutionplus/microservice",
    "description": "a laravel package to deal with specific case of micro-services connection. So, it doesn't suit all needs or handle all connection cases",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Ahmed Mabrouk",
            "email": "a.mabrouk@solutionplus.net"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/framework": "^8|^9|^10"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^9.5.10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Solutionplus\\MicroService\\MicroServiceServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Solutionplus\\MicroService\\": "src/",
            "Database\\Factories\\": "src/database/factories/",
            "Database\\Seeders\\": "src/database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
